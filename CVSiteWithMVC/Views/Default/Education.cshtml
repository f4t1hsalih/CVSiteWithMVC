@model List<CVSiteWithMVC.Models.Entity.tbl_education>

@if (Model != null)
{
    <div class="my-auto">
        <h2 class="mb-5">Eğitimlerim</h2>
        @foreach (var item in Model)
        {
            <div class="resume-item d-flex flex-column flex-md-row mb-5">
                <div class="resume-content mr-auto">
                    <h3 class="mb-0">@item.edc_title</h3>
                    <div class="subheading mb-3">@item.edc_subtitle</div>
                    <div>@item.edc_description</div>
                    <p>GNO: @item.edc_note</p>
                </div>
                <div class="resume-date text-md-right">
                    <span class="text-primary">@item.edc_date</span>
                </div>
            </div>
        }

    </div>
}
else
{
    <p>Model is null</p>
}