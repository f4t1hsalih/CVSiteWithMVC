@model List<CVSiteWithMVC.Models.Entity.tbl_experience>

@if (Model != null)
{
    <div class="my-auto">
        <h2 class="mb-5">Deneyimlerim</h2>
        @foreach (var item in Model)
        {
            <div class="resume-item d-flex flex-column flex-md-row mb-5">
                <div class="resume-content mr-auto">
                    <h3 class="mb-0">@item.exp_title</h3>
                    <div class="subheading mb-3">@item.exp_subtitle</div>
                    <p>@item.exp_description</p>
                </div>
                <div class="resume-date text-md-right">
                    <span class="text-primary">@item.exp_date</span>
                </div>
            </div>
        }
    </div>
}
else
{
    <p>Model is null</p>
}
